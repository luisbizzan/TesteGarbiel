@model FWLog.Web.Backoffice.Models.BORecebimentoNotaCtx.BORegistroRecebimentoViewModel
@{
Layout = null;

@BundleHelper.RenderViewScriptModal()
}
                        
<div class="col-sm-12 col-sm-6">
    <div class="form-group">
        @Html.Label("Entrada")
        @Html.TextBoxFor(m => m.DataRecebimento, new { @class = "form-control", @readonly = "readonly" })
