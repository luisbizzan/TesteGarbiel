    <div class="form-group">
        @Html.Label("Fornecedor")
        @Html.TextBoxFor(m => m.FornecedorNome, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>
<div class="col-sm-12 col-sm-6">
    <div class="form-group">
        @Html.Label("Nota/Série")
