    </div>
</div>
<div class="col-sm-12 col-sm-6">
    <div class="form-group">
        @Html.Label("Hora")
        @Html.TextBoxFor(m => m.HoraRecebimento, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>
