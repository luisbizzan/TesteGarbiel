@model FWLog.Web.Backoffice.Models.GarantiaCtx.GarantiaRemessa
@{
    Layout = null;
    @BundleHelper.RenderViewScriptModal()
}
<div class="modal-dialog modal-lg" style="width:95% !important;">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">
                <span aria-hidden="true">×</span>
            </button>
            <h4 class="modal-title">Criar Remessa</h4>
        </div>
        <div class="modal-body" style="overflow: auto; max-height: calc(100vh - 150px);">
            <div class="row">
                <div class="col-md-12">
                    <div class="x_panel">
                        <div class="x_content">
                            <form id="formRemessaCriar">

                                <div class="row divTipo" id="div1">
                                    <div class="col-xs-12 col-md-2 col-lg-3 form-group">
                                        @Html.LabelFor(m => m.Cod_Fornecedor)
                                        @Html.TextBoxFor(m => m.Cod_Fornecedor, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-2 col-lg-3 form-buttons">
                                    <button style="margin-top: 20px;" type="submit" class="btn btn-primary">Criar</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>